#
#  PRE: subrequest
#

# Cancel in subrequest
subrequest Access-Request {
	%(cancel:)
	test_fail
}

# Cancel in nested subrequest
subrequest Access-Request {
	subrequest Access-Request {
		%(cancel:)
		test_fail
	}
}

# Cancel parent with active subrequest, will run on next timer service
%(cancel:100ms)
subrequest Access-Request {
	%(delay_10s:1s)
}

test_fail

